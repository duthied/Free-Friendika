#: src/Module/Admin/Site.php:480
msgid "Mobile system theme"
msgstr "モバイルシステムのテーマ"

#: src/Module/Admin/Site.php:480
msgid "Theme for mobile devices"
msgstr "モバイルデバイスのテーマ"

#: src/Module/Admin/Site.php:481
msgid "Force SSL"
msgstr "SSLを強制する"

#: src/Module/Admin/Site.php:481
