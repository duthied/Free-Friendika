<?xml version="1.0" encoding="utf-8" ?>
<phpdocumentor xmlns="https://www.phpdoc.org" configVersion="3.0">
	<paths>
		<output>../doc/api</output>
		<cache>../doc/cache</cache>
	</paths>
	<version number="3.0">
		<api>
			<source dsn="../">
				<path>src</path>
				<path>mod</path>
				<path>include</path>
				<path>static</path>
				<path>bin</path>
				<path>view</path>
			</source>
			<ignore>
				<path>vendor/**/*</path>
				<path>asset/**/*</path>
				<path>bin/dev/**/*</path>
			</ignore>
		</api>
	</version>
</phpdocumentor>
